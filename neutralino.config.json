{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "chrome",
  "documentRoot": "/public/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "os.*",
    "app.*",
    "filesystem.*",
    "window.*",
    "extensions.*",
    "custom.*",
    "computer.*"
  ],
  "globalVariables": {
    "TEST_1": 1,
    "TEST_2": null,
    "TEST_3": 3.5,
    "TEST_4": [3, 5, 4, 5],
    "TEST_5": {
        "key": "value",
        "anotherKey": 100
    }
  },
  "modes": {
    "window": {
      "title": "Notes",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/public/thorium.png",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "Notes",
    "resourcesPath": "/public/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.14.1",
    "clientVersion": "3.12.0",
    "frontendLibrary": {
      "patchFile" : "/public/index.html",
      "devUrl" : "http://localhost:8080"
    }
  },
  "enableExtensions": true,
  "extensions": [
    {
      "id": "database.service",
      "command": "node ${NL_PATH}/extensions/db/dist/index.js"
    },
    {
      "id" : "codeblock.manager",
      "command": "node ${NL_PATH}/extensions/code-block/dist/index.js"
    },
    {
      "id" : "extensions.manager",
      "command": "node ${NL_PATH}/extensionsManager/dist/index.js"
    }
  ]
}